<launch>
    <param name="/use_sim_time" value="true"/>
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find socspioneer)/config/pf.rviz"/>
    <node name="stage" pkg="stage_ros" type="stageros" args="--clock $(find socspioneer)/data/meeting.world"/>
    <node name="map_server" pkg ="map_server" type = "map_server" args= "$(find socspioneer)/maps/map.yaml"/>
        <param name="transform_publish_period" value="0.08"/>

</launch>
